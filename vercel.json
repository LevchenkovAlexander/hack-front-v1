{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ks4extydw.localto.net/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "localtonet-skip-warning",
          "value": "true"
        }
      ]
    }
  ]
}